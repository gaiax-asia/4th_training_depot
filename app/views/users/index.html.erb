<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>

<h1>Listing Users</h1>
<%= will_paginate @users %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user %></td>
        
      </tr>
    <% end %>
    <%= link_to 'Edit Profile', edit_user_registration_path%>
  </tbody>
</table>
<%= will_paginate @users %>

<br>
